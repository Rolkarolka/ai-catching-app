<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <rotate android:fromDegrees="45">
            <shape android:shape="rectangle">
                <stroke
                    android:width="1dp"
                    android:color="@color/black" />
                <solid android:color="@color/pink" />
                <size
                    android:width="40dp"
                    android:height="40dp" />
            </shape>
        </rotate>
    </item>
    <item
        android:left="20dp"
        android:right="20dp"
        android:top="15dp"
        android:bottom="15dp"
        android:drawable="@drawable/ic_more_user_details"/>
</layer-list>
